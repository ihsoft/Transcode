{
  "profile_name": "Movies",
  "dry_run": false,
  "stdout_loglevel": 2,
  "file_log_name": "Transcode_movies.log",
  "file_loglevel": 3,
  "ffmpeg_path": "bin/ffmpeg",
  "ffmpeg_global_opts": [],
  "ffprobe_path": "bin/ffprobe",
  "scan_folders": ["z:/Movies"],
  "scan_patterns": ["^.*\\.+mkv$"],
  "target_folder": "",
  "make_own_folder": "subs",
  "cleanup_mode": "move",
  "cleanup_move_folder": "z:/Remuxed",
  "vcodec_type": "h264",
  "#vcodec_transcode": ["libx264", "-x264opts"],
  "vcodec_transcode": [],
  "acodec_type": "aac",
  "acodec_lang": "rus",
  "acodec_fallback_to_default": true,
  "acodec_dubbing_strings": ["dubbing", "дубляж"],
  "acodec_channels": 2,
  "acodec_transcode": ["aac", "-b:a", "112k", "-ac", "2", "-pass", "2"],
  "scodec": "srt",
  "scodec_lang": "rus",
  "scodec_forced_strings": ["forced", "форсированные", "forsed"],
  "save_cover_image": true
}
